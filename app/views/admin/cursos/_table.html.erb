<%- model_class=Curso -%>





<div class="accordion" id="accordionExample">
  <div class="accordion-item">
    <h2 class="accordion-header">
      <button class="accordion-button" type="button" data-bs-toggle="collapse" data-bs-target="#Curso" aria-expanded="true" aria-controls="Curso">
        <%= model_class.model_name.human(count: 2).titleize %>
      </button>
    </h2>
    <div id="Curso" class="accordion-collapse collapse <%= show_collapse %>" data-bs-parent="#accordionExample">
      <div class="accordion-body">
        <%= render partial: "cursos/datatable" , locals: {remote: remote} %>
      </div>
    </div>
  </div>
</div>